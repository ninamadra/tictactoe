<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<StackPane xmlns="http://javafx.com/javafx/8"
           xmlns:fx="http://javafx.com/fxml/1"
           fx:controller="com.example.tictactoe.MenuController"
           prefHeight="10.0" prefWidth="300.0">
    <MenuBar StackPane.alignment="CENTER">
        <Menu text="menu">
            <MenuItem text="exit" onAction="#menuClickHandler"/>
            <MenuItem text="help" onAction="#menuClickHandler"/>
        </Menu>
    </MenuBar>
</StackPane>
